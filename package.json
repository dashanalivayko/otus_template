{
  "name": "otus_template",
  "version": "1.0.0",
  "description": "Start repo with required configuration",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV='production' webpack",
    "start": "webpack-dev-server",
    "test": "jest",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "lint:fix": "npm run lint -- --fix",
    "prettify": "npx prettier src -c",
    "prettify:fix": "npm run prettify -- --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
